{
  "expo": {
    "name": "LifeLink",
    "slug": "LifeLink",
    "version": "1.0.0",
    "icon": "./assets/images/Lifelink_splash.png",
    "orientation": "portrait",
    "scheme": "lifelink",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.adepusricharan-03.LifeLink",
      "icon": "./assets/images/Lifelink_splash.png"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Lifelink_splash.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.adepusricharan_03.LifeLink"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/Lifelink_splash.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Lifelink_splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "preventAutoHide": true
        }
      ],
      "expo-secure-store",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.0.21",
            "usesCleartextTraffic": true,
            "enableProguardInReleaseBuilds": true,
            "enableShrinkResourcesInReleaseBuilds": true,
            "useLegacyPackaging": true,
            "enableMinifyInReleaseBuilds": true
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
